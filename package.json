{
  "name": "pixiv.moe",
  "version": "1.6.1",
  "description": "A pinterest-style layout site, shows illusts on [pixiv.net](http://pixiv.net) order by popularity. Written with React.",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "prepare": "husky install",
    "clean": "rimraf dist/*",
    "predist": "npm run clean",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-server.json\" webpack --config build/webpack.config.ts",
    "postbuild": "copyfiles -f ./src/icons/*.ico ./src/icons/*.png ./dist/assets && copyfiles -f ./src/manifest.json ./CNAME ./dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx --fix src",
    "dev": "cross-env NODE_ENV=development PIXIV_API_ENTRY=http://localhost:3000/pixiv TS_NODE_PROJECT=\"tsconfig-server.json\" webpack-dev-server --config build/webpack.config.ts",
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-server.json\" webpack-dev-server --config build/webpack.config.ts",
    "type:check": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kokororin/pixiv.moe.git"
  },
  "keywords": [
    "pixiv",
    "lovelive"
  ],
  "author": "kokororin",
  "license": "MIT",
  "homepage": "https://github.com/kokororin/pixiv.moe#readme",
  "bugs": {
    "url": "https://github.com/kokororin/pixiv.moe/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.4.0",
    "@types/jszip": "^3.4.1",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-intl": "^3.0.0",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-stack-grid": "^0.7.2",
    "@types/shortid": "^0.0.29",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-merge": "^5.0.0",
    "@types/workbox-webpack-plugin": "^5.1.8",
    "all-contributors-cli": "^6.13.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-kotori": "^0.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "prettier": "~2.0.4",
    "react-hot-loader": "4.13.0",
    "rimraf": "^3.0.2",
    "simple-progress-webpack-plugin": "^2.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-manifest-plugin": "^4.1.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@hot-loader/react-dom": "~17.0.1",
