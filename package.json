    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^5.0.3",
    "@mui/styles": "^5.0.1",
    "ahooks": "^3.1.9",
    "animation-frame": "^0.3.0",
    "classnames": "^2.3.1",
    "core-js": "^3.20.3",
    "dayjs": "^1.10.7",
    "honoka": "^0.5.2",
    "inobounce": "^0.2.0",
    "is-mobile": "^3.0.0",
    "jszip": "^3.7.1",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.6",
    "react": "~17.0.2",
    "react-adsense-google": "^1.3.1",
    "react-combine-provider": "^0.1.3",
    "react-dom": "~17.0.2",
    "react-ga": "^3.3.0",
    "react-image": "^4.0.3",
    "react-intl": "^5.24.4",
    "react-metatags-hook": "^1.3.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-stack-grid": "^0.7.1",
    "regenerator-runtime": "^0.13.9",
    "shortid": "^2.2.16",
    "socket.io-client": "^4.1.3",
    "store2": "^2.13.1",
    "supports-webp": "^2.0.1",
    "url-search-params-polyfill": "^8.1.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
